
<p id="notice"><%= notice %></p>

<!-- Page Content -->
<div class="container">
	<h5>Latest Post</h5>

	<div class="row">
		<% @logs.each do |log| %>
			<div class="card-deck col-lg-3 col-sm-6">
				<div class="card h-100">
					<div class="log-list-card">
						<a href="/logs/<%= log.id %>">
							<img class="card-img-top" src="<%= log.img %>" />
						</a>
					</div>
					<div class="card-body">
						<h5 class=card-title>
							<%= log.location.titlecase %>
						</h5>
							<p class="card-text-posted-by">
								<%= log.participant.titlecase %>
							</p>
							<p class="card-text-timestamp">
								<%= log.log_date %>
							</p>
							<p class="card-text">
								<%= log.observation %>
							</p>
					</div>
				</div>
			</div>
		<% end %>
	</div>

</div><!-- end of container -->