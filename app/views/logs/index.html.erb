<p id="notice"><%= notice %></p>

<h1>Logs</h1>

<table>
  <thead>
    <tr>
      <th>Log date</th>
      <th>Log time</th>
      <th>Location</th>
      <th>Participant</th>
      <th>Sensor type</th>
      <th>Reading</th>
      <th>Observation</th>
      <th>Img</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.log_date %></td>
        <td><%= log.log_time %></td>
        <td><%= log.location %></td>
        <td><%= log.participant %></td>
        <td><%= log.sensor_type %></td>
        <td><%= log.reading %></td>
        <td><%= log.observation %></td>
        <td><%= log.img %></td>
        <td><%= link_to 'Show', log %></td>
        <td><%= link_to 'Edit', edit_log_path(log) %></td>
        <td><%= link_to 'Destroy', log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Log', new_log_path %>
